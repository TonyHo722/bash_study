gits=git status
gitst=git stash
gitd=git diff
gitb=git branch
gitbd=git branch -d
gitba=git branch -a
gitbva=git branch -v -a
gitr=git remote -v
gitl=git log -5
gitln=git log --name-status
gitl1=git log -1
gitl1n=git log -1 --name-status
gitl5=git log --oneline -5
gitl20=git log --oneline -20
gitldog=git log --decorate --oneline --graph
gitladog=git log --all --decorate --oneline --graph
gitlafn=git log --all --full-history --name-status


